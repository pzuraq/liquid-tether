{
  "name": "liquid-tether",
  "version": "2.0.7",
  "description": "Modals made easy",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/pzuraq/liquid-tether",
  "license": "MIT",
  "author": "Christopher Garrett <cmg@pzuraq.co>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.3.0",
    "liquid-wormhole": "^5.0.2",
    "tether": "^1.4.7"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.3.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^2.9.6",
    "@embroider/test-setup": "^1.8.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "bootstrap-sass": "~3.3.7",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.12.0",
    "ember-cli": "~4.0.1",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-markdown-compiler": "github:RobbieTheWagner/ember-cli-markdown-compiler#930f9605920b90cb0ddd447750394f68b4ef72dd",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.1.0",
    "ember-source": "~3.28.12",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.16.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-ember": "^10.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.5",
    "eslint-plugin-qunit": "^7.3.4",
    "liquid-fire": "^0.37.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "qunit": "^2.24.3",
    "qunit-dom": "^2.0.0",
    "sass": "^1.94.2",
    "velocity-animate": "^1.5.2",
    "webpack": "^5.103.0"
  },
  "peerDependencies": {
    "liquid-fire": "^0.37.1",
    "velocity-animate": "^1.5.2"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "liquid-fire"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "core-js",
      "fsevents",
      "highlight.js"
    ]
  }
}
